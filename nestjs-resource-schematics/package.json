{
  "name": "nestjs-resource-schematics",
  "version": "0.0.1",
  "main": "dist/index.js",
  "schematics": "./collection.json",
  "scripts": {
    "build": "tsc -p tsconfig.json && npm run copy",
    "copy": "cp -r src/resource/files dist/resource/files\n cp src/resource/schema.json dist/resource/schema.json\n"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "^18.0.0",
    "@angular-devkit/core": "^18.0.0",
    "typescript": "^5.0.0"
  }
}
