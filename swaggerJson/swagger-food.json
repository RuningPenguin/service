        "bearerFormat": "JWT",
        "type": "http",
        "description": "请输入 JWT Token（示例：eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.xxx）"
      }
    },
    "schemas": {
      "ApiOkResponseRes": {
        "type": "object",
        "properties": {
          "code": {
            "type": "number",
            "example": 0
          },
          "data": {
            "type": "object"
          },
          "message": {
            "type": "string",
            "example": "success"
          },
          "path": {
            "type": "string",
            "example": "/api/user/list"
          },
          "timestamp": {
            "type": "string",
            "example": "2025-01-01T00:00:00.000Z"
          }
        },
        "required": [
          "code",
          "data",
          "message",
          "path",
          "timestamp"
        ]
      },
      "ResClassifyDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "分类名称"
          }
        },
        "required": [
          "name"
        ]
      },
      "AddClassifyDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "分类名称"
          }
        },
        "required": [
          "name"
        ]
      },
      "UpdateClassifyDto": {
        "type": "object",
        "properties": {}
      },
      "AddDishDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "菜品名称"
          },
          "price": {
            "type": "number",
            "description": "菜品价格（支持两位小数）",
            "example": 15.99
          },
          "brief": {
            "type": "string",
            "description": "菜品简介"
          },
          "pic": {
            "type": "string",
            "description": "菜品封面图片"
          },
          "pics": {
            "type": "string",
            "description": "菜品主图片"
          },
          "detail": {
            "type": "string",
            "description": "菜品详情"
          },
          "classifyIds": {
            "description": "菜品分类id",
            "type": "array",
            "items": {
              "type": "number"
            }
          }
        },
        "required": [
          "name",
          "price",
          "brief",
          "pic",
          "pics",
          "detail",
          "classifyIds"
        ]
      },
      "DishListDto": {
        "type": "object",
        "properties": {
          "page": {
            "type": "number",
            "description": "页码，从 1 开始",
            "default": 1,
            "example": 1
          },
          "size": {
            "type": "number",
            "description": "每页数量，默认为 10",
            "default": 10,
            "example": 10
          },
          "classifyIds": {
            "type": "string",
            "description": "菜品分类id",
            "example": [
              1,
              2
            ]
          }
        },
        "required": [
          "page",
          "size",
          "classifyIds"
        ]
      },
      "UpdateDishDto": {
        "type": "object",
        "properties": {}
      }
